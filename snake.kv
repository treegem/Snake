#:kivy 1.10.1


<Snake>:
    size_hint: None, None
    size: 20, 20
    canvas:
        Color:
            rgb: 1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size

<SnakeGame>:
    head: snake_

    height: 600
    width: 800
    canvas.before:
        Color:
            rgba: 1, 1, 1, 0.1
        Rectangle:
            pos: self.pos
            size: self.size
        Color:
            rgb: 1,1,1

    Snake:
        id: snake_
        x: root.center_x
        y: root.center_y


